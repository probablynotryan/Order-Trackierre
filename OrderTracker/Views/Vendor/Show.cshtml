@{
  Layout = "_Layout";
}

@using OrderTracker.Models;

<h3>Here are all the items in this category:</h3>

<ol>
@foreach (Order order in @Model["order"])
{
  <li><a href='/vendors/@Model["vendor"].Id/orders/@order.Id'>@order.Description</a></li>
}
</ol>

<p><a href='/vendors/@Model["vendor"].Id/order/new'>Add a new item</a></p>
<p><a href='/vendors'>Return to category list</a></p>
<p><a href='/'>Return to Main Page</a></p>